""" Generic command line tool for Neo4j.
"""

from __future__ import print_function

import sys

from py2neo.tool import Tool

try:
    Tool().do(sys.argv)
    sys.exit(0)
except Exception as err:
    print(*err.args)
    sys.exit(1)
